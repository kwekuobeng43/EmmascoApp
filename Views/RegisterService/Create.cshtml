@model RegisterServiceDt;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<div class="card">
    <div class="card-header">
        Register To Continue
    </div>
    <div class="card-body">
        <h5 class="card-title">Register</h5>

        <form method="post">



            <div class="row mb-3">
                <label for="firstName" class="col-sm-2 col-form-label">First Name</label>
                <div class="col-sm-10">
                    <input type="text" class="form asp-for="FirstName" class="form-control" id="firstName" name="FirstName" />
                </div>
            </div>

            <div class="row mb-3">
                <label for="lastName" class="col-sm-2 col-form-label">Last Name</label>
                <div class="col-sm-10">
                    <input type="text" asp-for="LastName" class="form-control" id="lastName" name="LastName" />
                </div>
            </div>

            <div class="row mb-3">
                <label for="email" class="col-sm-2 col-form-label">Email</label>
                <div class="col-sm-10">
                    <input type="text" asp-for="Email" class="form-control" id="email" name="Email" />
                </div>
            </div>

            <div class="row mb-3">
                <label for="phone" class="col-sm-2 col-form-label">Phone</label>
                <div class="col-sm-10">
                    <input type="text" asp-for="Phone" class="form-control" id="phone" name="Phone" />
                </div>
            </div>

            <div class="row mb-3">
                <label for="address" class="col-sm-2 col-form-label">Address</label>
                <div class="col-sm-10">
                    <input type="text" asp-for="Address" class="form-control" id="address" name="Address" />
                </div>
            </div>

            <div class="row mb-5">
                <label for="carImage" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-8">

                    <button type="submit" class="btn btn-primary" asp-controller="RegisterService" asp-action="Create">SUBMIT</button>
                </div>
            </div>
        </form>
    </div>
</div>

