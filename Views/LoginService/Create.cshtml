@model LoginServiceDt;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div class="card">
    <div class="card-header">
        Login To Continue
    </div>
    <div class="card-body">
        <h5 class="card-title">Login</h5>

        <form method="post">
            <div class="row mb-3">
                <label for="userName" class="col-sm-2 col-form-label">User Name</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="userName" />
                </div>
            </div>

            <div class="row mb-3">
                <label for="userPassword" class="col-sm-2 col-form-label">Password</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="userPassword" />
                </div>
            </div>

            <div class="row mb-5">
                <label for="sub" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-8">

                    <button type="submit" class="btn btn-primary" asp-controller="LoginService" asp-action="Create">SUBMIT</button>
                </div>
            </div>

        </form>
    </div>
</div>




